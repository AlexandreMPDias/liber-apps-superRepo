{"wml.student": "cd ./student-app && iml add core containers native",
"wml.tutor": "cd ./tutor-app && iml add core containers native",
"wml.backbone": "yarn wml.native && yarn wml.containers",
"clear": "run-p clear.core clear.native clear.containers clear.student clear.tutor clear.metro clear.watch",
"dev": "run-p dev.native dev.containers",
"install.backbone": "run-p install.core install.containers install.native",
"install.all.tutor": "yarn install.backbone && yarn install.tutor",
"install.all.student": "yarn install.backbone && yarn install.student",
"install.all": "yarn install.backbone && yarn install.student && yarn install.tutor",
"fetch.all": "yarn fetch.native && yarn fetch.containers && yarn fetch.core && yarn fetch.tutor && yarn fetch.student",
