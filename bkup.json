"--------------------": "echo -----------------------",
"fetch.native": "cd ./components-native && git fetch -p",
"fetch.containers": "cd ./components-containers && git fetch -p",
"fetch.core": "cd ./core && git fetch -p",
"fetch.tutor": "cd ./tutor-app && git fetch -p",
"fetch.student": "cd ./student-app && git fetch -p",
"pull.native": "cd ./components-native && git pull",
"pull.containers": "cd ./components-containers && git pull",
"pull.core": "cd ./core && git pull",
"pull.tutor": "cd ./tutor-app && git pull",
"pull.student": "cd ./student-app && git pull",
"delay": "node ./node_modules/.bin/npm-delay",
"clear.watch": "wml rm all && watchman watch-del-all",
"clear.metro": "rm -rf /tmp/metro-*",
"clear.core": "rm -rf ./core/node_modules ./core/yarn.lock",
"clear.native": "rm -rf ./components-native/node_modules ./components-native/yarn.lock",
